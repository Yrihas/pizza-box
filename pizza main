local function IKLSJ_fake_script() -- TextButton_12.LocalScript 
	local script = Instance.new('LocalScript', TextButton_12)

	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
	
		local function colorCharacter(character)
			for _, part in ipairs(character:GetChildren()) do
				if part:IsA("BasePart") then
					part.BrickColor = BrickColor.new("Really black")
				elseif part:IsA("Accessory") then
					if part:FindFirstChild("Handle") then
						part.Handle.BrickColor = BrickColor.new("Really black")
					end
				end
			end
		end
	
		local function onPlayerAdded(player)
			player.CharacterAppearanceLoaded:Connect(function(character)
				colorCharacter(character)
			end)
		end
	
		-- Apply color change to all players currently in the game
		for _, player in ipairs(Players:GetPlayers()) do
			if player.Character then
				colorCharacter(player.Character)
			end
		end
	
		-- Connect to PlayerAdded event
		Players.PlayerAdded:Connect(onPlayerAdded)
	
	end)
end
