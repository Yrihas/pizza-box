local function KXIJ_fake_script() -- TextButton_10.LocalScript 
	local script = Instance.new('LocalScript', TextButton_10)

	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
	
		local function addFireToCharacter(character)
			-- Iterate through each part of the character
			for _, part in ipairs(character:GetDescendants()) do
				if part:IsA("BasePart") then
					-- Create a Fire instance
					local fire = Instance.new("Fire")
					fire.Size = 15  -- Adjust size of the fire
					fire.Heat = 10  -- Adjust heat intensity of the fire
					fire.Parent = part
				end
			end
		end
	
		local function onPlayerAdded(player)
			player.CharacterAdded:Connect(function(character)
				character:WaitForChild("Humanoid")
				addFireToCharacter(character)
			end)
		end
	
		-- Apply fire to all players currently in the game
		for _, player in ipairs(Players:GetPlayers()) do
			if player.Character then
				addFireToCharacter(player.Character)
			end
			player.CharacterAdded:Connect(onPlayerAdded)
		end
	
		-- Connect to PlayerAdded event
		Players.PlayerAdded:Connect(onPlayerAdded)
	
	end)
end
