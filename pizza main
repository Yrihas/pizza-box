local function KLJFZT_fake_script() -- ESP.LocalScript 
	local script = Instance.new('LocalScript', ESP)

	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
		local TweenService = game:GetService("TweenService")
	
		-- Function to create ESP for a character
		local function createESP(character)
			local parts = {"Head", "Left Arm", "Right Arm", "Left Leg", "Right Leg", "Torso"}
	
			for _, partName in ipairs(parts) do
				local part = character:FindFirstChild(partName) or character:FindFirstChild(partName:sub(1, 1).."Lower"..partName:sub(2)) -- For R15 support
				if part then
					-- Create a BillboardGui
					local billboardGui = Instance.new("BillboardGui")
					billboardGui.Name = "ESP"
					billboardGui.Size = UDim2.new(4, 0, 4, 0)
					billboardGui.AlwaysOnTop = true
	
					-- Create a Frame inside the BillboardGui
					local frame = Instance.new("Frame")
					frame.Size = UDim2.new(1, 0, 1, 0)
					frame.BackgroundTransparency = 0.5
					frame.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
					frame.Parent = billboardGui
	
					-- Attach the BillboardGui to the part
					billboardGui.Adornee = part
					billboardGui.Parent = part
	
					-- Tween the frame background color to create a rainbow shimmer effect
					local tweenInfo = TweenInfo.new(2, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true)
					local tween = TweenService:Create(frame, tweenInfo, {BackgroundColor3 = Color3.fromHSV(tick() % 5 / 5, 1, 1)})
					tween:Play()
				end
			end
	
			-- Create a BillboardGui for the player's name
			local nameBillboard = Instance.new("BillboardGui")
			nameBillboard.Name = "NameESP"
			nameBillboard.Size = UDim2.new(0, 200, 0, 50)
			nameBillboard.StudsOffset = Vector3.new(0, 3, 0)
			nameBillboard.AlwaysOnTop = true
	
			local nameLabel = Instance.new("TextLabel")
			nameLabel.Size = UDim2.new(1, 0, 1, 0)
			nameLabel.BackgroundTransparency = 1
			nameLabel.Text = character.Name
			nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
			nameLabel.TextStrokeTransparency = 0.5
			nameLabel.Font = Enum.Font.SourceSansBold
			nameLabel.TextSize = 14
			nameLabel.Parent = nameBillboard
	
			nameBillboard.Adornee = character:WaitForChild("Head")
			nameBillboard.Parent = character:WaitForChild("Head")
	
			-- Tween the name label text color to create a rainbow shimmer effect
			local nameTween = TweenService:Create(nameLabel, tweenInfo, {TextColor3 = Color3.fromHSV(tick() % 5 / 5, 1, 1)})
			nameTween:Play()
		end
	
		local function onPlayerAdded(player)
			player.CharacterAdded:Connect(function(character)
				character:WaitForChild("Humanoid")
				createESP(character)
			end)
		end
	
		-- Apply ESP to all players currently in the game
		for _, player in ipairs(Players:GetPlayers()) do
			if player.Character then
				createESP(player.Character)
			end
			player.CharacterAdded:Connect(createESP)
		end
	
		-- Connect to PlayerAdded event
		Players.PlayerAdded:Connect(onPlayerAdded)
	
	end)
end
coroutine.wrap(KLJFZT_fake_script)()
