local function TTUK_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		-- Settings
		local decalId = "http://www.roblox.com/asset/?id=10132220101" -- Replace with your decal ID
	
		-- Function to apply decals to a character
		local function applyDecals(character)
			for _, part in ipairs(character:GetDescendants()) do
				if part:IsA("BasePart") then
					-- Check if a decal already exists to prevent duplication
					if not part:FindFirstChildOfClass("Decal") then
						local decal = Instance.new("Decal")
						decal.Texture = decalId
						decal.Face = Enum.NormalId.Front
						decal.Parent = part
					end
				end
			end
		end
	
		-- Apply decals to existing players' characters
		for _, player in ipairs(game.Players:GetPlayers()) do
			if player.Character then
				applyDecals(player.Character)
			end
			player.CharacterAdded:Connect(applyDecals)
		end
	
		-- Apply decals to newly joined players
		game.Players.PlayerAdded:Connect(function(player)
			player.CharacterAdded:Connect(applyDecals)
		end)
	
	end)
end
coroutine.wrap(TTUK_fake_script)()
local function KUVL_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		-- Settings
		local rainbowSpeed = 0.5 -- Speed of the color change
	
		-- Helper function to get a color from HSV
		local function HSVToRGB(h, s, v)
			local r, g, b
	
			local i = math.floor(h * 6)
			local f = h * 6 - i
			local p = v * (1 - s)
			local q = v * (1 - f * s)
			local t = v * (1 - (1 - f) * s)
	
			i = i % 6
	
			if i == 0 then
				r = v
				g = t
				b = p
			elseif i == 1 then
				r = q
				g = v
				b = p
			elseif i == 2 then
				r = p
				g = v
				b = t
			elseif i == 3 then
				r = p
				g = q
				b = v
			elseif i == 4 then
				r = t
				g = p
				b = v
			elseif i == 5 then
				r = v
				g = p
				b = q
			end
	
			return Color3.new(r, g, b)
		end
	
		-- Function to apply rainbow colors to a character
		local function applyRainbowColors(character, h)
			local color = HSVToRGB(h, 1, 1)
	
			for _, part in ipairs(character:GetDescendants()) do
				if part:IsA("BasePart") then
					part.Color = color
				elseif part:IsA("Accessory") and part:FindFirstChild("Handle") then
					part.Handle.Color = color
				end
			end
		end
	
		-- Main script
		local h = 0
		game:GetService("RunService").RenderStepped:Connect(function(deltaTime)
			h = (h + rainbowSpeed * deltaTime) % 1
			for _, player in ipairs(game.Players:GetPlayers()) do
				if player.Character and player.Character:FindFirstChild("Humanoid") then
					applyRainbowColors(player.Character, h)
				end
			end
		end)
	
	end)
end
coroutine.wrap(KUVL_fake_script)()
