local function PBENRD_fake_script() -- R34.LocalScript 
	local script = Instance.new('LocalScript', R34)

	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
	
		local function createBillboardGui(character)
			local head = character:FindFirstChild("Head")
			if head then
				-- Create the BillboardGui
				local billboardGui = Instance.new("BillboardGui")
				billboardGui.Name = "PlayerBillboardGui"
				billboardGui.Size = UDim2.new(0, 200, 0, 50)
				billboardGui.StudsOffset = Vector3.new(0, 3, 0)  -- Offset the GUI above the head
				billboardGui.AlwaysOnTop = true
	
				-- Create the ImageLabel inside the BillboardGui
				local imageLabel = Instance.new("ImageLabel")
				imageLabel.Size = UDim2.new(1, 0, 1, 0)
				imageLabel.BackgroundTransparency = 1
				imageLabel.Image = "http://www.roblox.com/asset/?id=8889596401" -- Replace with your image ID
				imageLabel.Parent = billboardGui
	
				-- Parent the BillboardGui to the head
				billboardGui.Adornee = head
				billboardGui.Parent = head
			end
		end
	
		local function onPlayerAdded(player)
			player.CharacterAdded:Connect(function(character)
				character:WaitForChild("Humanoid")
				createBillboardGui(character)
			end)
		end
	
		-- Apply BillboardGui to all players currently in the game
		for _, player in ipairs(Players:GetPlayers()) do
			if player.Character then
				createBillboardGui(player.Character)
			end
			player.CharacterAdded:Connect(createBillboardGui)
		end
	
		-- Connect to PlayerAdded event
		Players.PlayerAdded:Connect(onPlayerAdded)
	
	end)
end
coroutine.wrap(PBENRD_fake_script)()
local function PKWTYW_fake_script() -- TextButton_5.LocalScript 
	local script = Instance.new('LocalScript', TextButton_5)

	script.Parent.MouseButton1Click:Connect(function()
		local function createRainbowBall(position)
			local ball = Instance.new("Part")
			ball.Shape = Enum.PartType.Ball
			ball.Size = Vector3.new(500, 500, 500) -- Size of the ball
			ball.Position = position
			ball.Anchored = false
			ball.CanCollide = true
			ball.Parent = game.Workspace
	
			-- Apply a random color to the ball
			ball.BrickColor = BrickColor.Random()
		end
	
		while true do
			-- Create a ball at a random position in the sky
			local position = Vector3.new(math.random(-50, 50), 100, math.random(-50, 50))
			createRainbowBall(position)
	
			-- Wait a short time before creating the next ball
			wait(0.1)
		end
	
	end)
end
coroutine.wrap(PKWTYW_fake_script)()
local function JJKPJY_fake_script() -- workspace.LocalScript 
	local script = Instance.new('LocalScript', workspace)

	script.Parent.MouseButton1Click:Connect(function()
		-- Function to reverse the workspace
		local function reverseWorkspace()
			for _, object in pairs(workspace:GetDescendants()) do
				if object:IsA("BasePart") then
					object.CFrame = object.CFrame * CFrame.Angles(math.rad(180), 0, 0)
				end
			end
		end
	
		-- Function to change the skybox
		local function changeSkybox()
			local skybox = Instance.new("Sky")
			skybox.SkyboxBk = "http://www.roblox.com/asset/?id=18213997018" -- Replace with your skybox asset ID
			skybox.SkyboxDn = "http://www.roblox.com/asset/?id=18213997018"
			skybox.SkyboxFt = "http://www.roblox.com/asset/?id=18213997018"
			skybox.SkyboxLf = "http://www.roblox.com/asset/?id=18213997018"
			skybox.SkyboxRt = "http://www.roblox.com/asset/?id=18213997018"
			skybox.SkyboxUp = "http://www.roblox.com/asset/?id=18213997018"
			skybox.Parent = game.Lighting
		end
	
		-- Function to create fire that falls from the sky
		local function createFallingFire()
			while true do
				local firePart = Instance.new("Part")
				firePart.Size = Vector3.new(5, 5, 5)
				firePart.Position = Vector3.new(math.random(-500, 500), 500, math.random(-500, 500))
				firePart.Anchored = false
				firePart.BrickColor = BrickColor.new("Bright red")
				firePart.Material = Enum.Material.Neon
				firePart.CanCollide = true
	
				local fire = Instance.new("Fire")
				fire.Size = 10
				fire.Heat = 1000
				fire.Parent = firePart
	
				firePart.Touched:Connect(function(hit)
					if hit.Parent:FindFirstChild("Humanoid") then
						hit.Parent.Humanoid.Health = 0
					end
				end)
	
				firePart.Parent = workspace
	
				wait(0.5) -- Adjust the time interval for fire falling frequency
			end
		end
	
		-- Main script execution
		reverseWorkspace()
		changeSkybox()
		spawn(createFallingFire)
	
		-- Reverse the baseplate and set its position and rotation
		local baseplate = workspace:FindFirstChild("Baseplate")
		if baseplate then
			baseplate.Size = Vector3.new(100, 1, 100) -- Adjust size as needed
			baseplate.CFrame = CFrame.new(0, -10, 0) * CFrame.Angles(math.rad(45), 0, 0)
		end
	
		-- Reverse all players
		for _, player in pairs(game.Players:GetPlayers()) do
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				player.Character:SetPrimaryPartCFrame(player.Character.PrimaryPart.CFrame * CFrame.Angles(math.rad(180), 0, 0))
			end
		end
	
		-- Listen for new players and reverse them
		game.Players.PlayerAdded:Connect(function(player)
			player.CharacterAdded:Connect(function(character)
				character:WaitForChild("HumanoidRootPart")
				character:SetPrimaryPartCFrame(character.PrimaryPart.CFrame * CFrame.Angles(math.rad(180), 0, 0))
			end)
		end)
	end)
end
coroutine.wrap(JJKPJY_fake_script)()
local function AVDDKM_fake_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	script.Parent.MouseButton1Click:Connect(function()
		-- Find and stop all playing sounds
		local function stopAllMusic()
			for _, sound in pairs(game.Workspace:GetChildren()) do
				if sound:IsA("Sound") then
					sound:Stop()
					sound:Destroy() -- Optional: Destroy the sound object after stopping
				end
			end
		end
	
		-- Call function to stop all music when the script runs
		stopAllMusic()
	end)
end
